    "GR_004_4": "\r\n\r\n/*4-4*/\r\n.GR_004_4{color: #22223b; font-size:21px;  }\r\n.GR_004_4 > table > thead{font-size:18px; color:#7a7c8b;  }\r\n.GR_004_4 > table > thead >tr{background:#f7f7fc; }\r\n.GR_004_4 > table > thead >tr:first-child{background:url('./img/ic_shadow.png') right top repeat-x, #f7f7fc;background-size: 2px 10px, 100% 100%;  }\r\n.GR_004_4 > table > thead > tr{height: 40px; }\r\n.GR_004_4 > table > thead > tr > td{border: solid 1px #e4e5ec;  }\r\n.GR_004_4 > table > thead > tr > td:first-child{border-left: none;  } \r\n.GR_004_4 > table > thead > tr > td:last-child{border-right: none;} \r\n.GR_004_4 > div > table > tbody > tr{height: 50px; }\r\n.GR_004_4 > div > table > tbody > tr > td{/*border: solid 1px #e4e5ec;*/  }\r\n.GR_004_4 > div > table > tbody > tr > td:first-child{border-left: none;} \r\n.GR_004_4 > div > table > tbody > tr > td:last-child{border-right: none;}  \r\n.GR_004_4 > div > table > tbody > tr:nth-child(2n) > td{border-bottom: solid 1px #c6c6ce;}\r\n.GR_004_4 > div > table > tbody > tr:nth-child(2n-1) > td:last-child{border-bottom: solid 1px #c6c6ce;}",
    "GR_004_5": "\r\n\r\n/*4-5 상단 요약 그리드*/\r\n/*.GR_004_5{color: #fff;  font-size:23px; }\r\n.GR_004_5 > table > tbody > tr{height: 55px;}\r\n.GR_004_5 > table > tbody > tr:first-child > td{border-top: solid 1px rgba(128,145,224,0.5);}\r\n.GR_004_5 > table > tbody > tr:last-child > td{border-bottom: solid 1px rgba(128,145,224,0.5);}\r\n.GR_004_5 > table > tbody > tr > td{border-bottom: solid 1px rgba(128,145,224,0.3); text-align: right; padding-right: 15px;}\r\n.GR_004_5 > table > tbody > tr > td:last-child{border-right: none;}\r\n.GR_004_5 > table > tbody > tr > td:nth-child(2n-1){color:rgba(255,255,255, 0.6); background-color: rgba(128,145,224,0.2); text-align:center; padding:0px; font-size:16px;}*/\r\n\r\n.GR_004_5{color: #fff; }\r\n.GR_004_5 > tr{height: 55px;}\r\n.GR_004_5 > tr:first-child > td{border-top: solid 1px rgba(128,145,224,0.5);}\r\n.GR_004_5 > tr:last-child > td{border-bottom: solid 1px rgba(128,145,224,0.5);}\r\n.GR_004_5 > tr > td{border-bottom: solid 1px rgba(128,145,224,0.3); text-align: right; padding-right: 15px;}\r\n.GR_004_5 > tr > td:last-child{border-right: none;}\r\n.GR_004_5 > tr > td:nth-child(2n-1){color:rgba(255,255,255, 0.6); background-color: rgba(128,145,224,0.2); text-align:center; padding:0px; font-family:'SemiBold';}",
    "GR_005": "\r\n\r\n\r\n/*5*/\r\n.GR_005{color: #22223b;  font-size:21px; }\r\n.GR_005 > div > table > tbody > tr{height: 65px;}\r\n.GR_005 > div > table > tbody > tr > td{border: solid 1px #e4e5ec; text-align: right; padding-right: 10px;}\r\n.GR_005 > div > table > tbody > tr > td:first-child{border-left: none; text-align:center; padding:0px;} \r\n.GR_005 > div > table > tbody > tr > td:last-child{border-right: none;}\r\n.GR_005 > div > table > tbody > tr > td:nth-child(4){text-align:center;padding:0px;}",
    "GR_006": "\r\n\r\n\r\n/*6*/\r\n.GR_006 {color: #ff4f62; font-size:20px; background-color: #272d42; }\r\n.GR_006 > tr{height: 40px;}\r\n.GR_006 > tr > td{border: solid 1px #303751; text-align: left; padding-left: 10px; text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);}\r\n.GR_006 > tr > td:nth-child(2){text-align:right;padding-right:10px;}\r\n.GR_006 > tr:nth-child(3) ~ tr{color: #008bff;}",
    "GR_001_2": "\r\n\r\n\r\n/*7  1*/\r\n.GR_001_2{color: #21213a; font-size:20px; background-color: #f3f3f8; }\r\n.GR_001_2 > div > table > tbody > tr > td { border-bottom: 1px solid #ffffff; border-top: 1px solid #ffffff; }\r\n.GR_001_2 > div > table > tbody > tr > td:nth-child(2) { border-right: 1px solid #ffffff;}",
    "GR_001_3": "\r\n\r\n/*7  1*/\r\n.GR_001_3{color: #22223b; font-size:21px; background-color: #f7f7fc; }\r\n.GR_001_3 > div > table {background-color: #f7f7fc;}\r\n.GR_001_3 > div > table > tbody > tr{height: 65px;}\r\n.GR_001_3 > div > table > tbody > tr:last-child > td{border-bottom: none;}\r\n.GR_001_3 > div > table > tbody > tr > td { text-align: right; border-bottom: 1px solid #e4e5ec;}",
    "GR_001": "\r\n\r\n/*1*/\r\n.GR_001{color: #22223b;  font-size:21px; }\r\n.GR_001 > div > table > tbody > tr{height: 67px;}\r\n.GR_001 > div > table > tbody > tr > td{border-top: solid 1px #e4e5ec; border-bottom: solid 1px #c6c6ce; text-align: right; padding-right: 20px;}\r\n.GR_001 > div > table > tbody > tr > td:nth-child(2){border-right: solid 1px #e4e5ec;} \r\n.GR_001 > div > table > tbody > tr > td:nth-child(1), .GR_001 > table > tbody > tr > td:nth-child(3){background: -webkit-linear-gradient(#e4e5ec, #e4e5ec) right center no-repeat, #ffffff; background-size: 1px 18px, 100% 100%;}",
    "GR_001_1": "\r\n\r\n/*1-1*/\r\n.GR_001_1{color: #ff3e47;  font-size:30px; background: #f7f7fc; }\r\n.GR_001_1 > div > table > tbody > tr{height: 65px;}\r\n.GR_001_1 > div > table > tbody > tr > td{border: solid 1px #e4e5ec;  padding-right: 10px; text-align: right;}\r\n.GR_001_1 > div > table > tbody > tr > td:first-child{border-left: none;} \r\n.GR_001_1 > div > table > tbody > tr > td:last-child{border-right: none;}\r\n.GR_001_1 > div > table > tbody > tr > td:nth-child(1){background-color: #e7f2ff; font-size:30px;}\r\n.GR_001_1 > div > table > tbody > tr:nth-child(10) ~ tr > td:nth-child(1){color: #007aff; background-color: #ffebed;}",
    "GR_002": "\r\n\r\n/*2*/\r\n.GR_002{color: #22223b;  font-size:20px; }\r\n.GR_002 > table > thead{background: #f7f7fc; color: #7a7c8b; font-size:18px;}\r\n.GR_002 > div > table > tbody > tr{height: 56px;}\r\n.GR_002 > div > table > thead > tr{height: 41px;}\r\n.GR_002 > div > table > tbody > tr:first-child td{border-top: solid 1px #c6c6ce;}\r\n.GR_002 > div > table > tbody > tr:nth-child(2n) > td{border-bottom: solid 1px #c6c6ce;}\r\n.GR_002 > div > table > tbody > tr > td, .GR_002 > table > thead > tr > td{border: solid 1px #e4e5ec; }\r\n.GR_002 > div > table > tbody > tr > td:first-child, .GR_002 > table > thead > tr > td:first-child{border-left: none;} \r\n.GR_002 > div > table > tbody > tr > td:last-child, .GR_002 > table > thead > tr > td:last-child{border-right: none;}",
    "GR_002_1": "  \r\n\r\n.GR_002_1{color: #22223b;  font-size:21px; background-color: #f7f7fc;}\r\n.GR_002_1 > div > table {background-color: #f7f7fc;}\r\n.GR_002_1 > div > table > tbody > tr{height: 40px;}\r\n.GR_002_1 > div > table > tbody > tr > td{border-right: solid 1px #e4e5ec; text-align: right; padding-right: 10px;}\r\n.GR_002_1 > div > table > tbody > tr > td:last-child{border-right: none;}",
    "CALEN": "\r\n\r\n\r\n\r\n/* 스마트 뷰*/\r\n.CALEN {color: #ffffff;  font-size:21px;}\r\n.CALEN thead{color: #8e8dff;}\r\n.CALEN tr{height: 55px; width: 55px;}\r\n.CALEN .B{color: #a9abbb; }\r\n.CALEN .T{color: #22223b; background:url('./img/ic_smart_calendar_bg.png') center center no-repeat, transparent;background-size: 50px 50px, 100% 100%; }\r\n.CALEN .O_H{color: #fb643c;}\r\n.CALEN .O_T{color: #22223b; background:url('./img/ic_calendar_bg.png') center center no-repeat, transparent;background-size: 50px 50px, 100% 100%; }\r\n.CALEN .O_T_TCH{color: #22223b; background:url('./img/ic_calendar_bg2.png') center center no-repeat, transparent;background-size: 50px 50px, 100% 100%; }",
    "CALEN2": "\r\n\r\n.CALEN2 {color: #5e637d;  font-size:21px;}\r\n.CALEN2 thead tr{height: 35px; }\r\n.CALEN2 thead td{color: #81818d ; height: 35px; background-color: #e4e5ec; border-top: solid 1px #c6c6ce; border-bottom: solid 1px #c6c6ce; font-size:15px; }\r\n.CALEN2 td:first-child, .CALEN2 td:last-child, .CALEN2 .H {color: #ff3552; }\r\n.CALEN2 tr{height: 155px;}\r\n.CALEN2 td{border-bottom: solid 1px #e4e5ec; border-right: solid 1px #e4e5ec; }\r\n.CALEN2 td:last-child{border-right: none;}\r\n.CALEN2 tbody td{vertical-align: top; padding-top: 20px;}\r\n.CALEN2 .T{color: #ffffff; background:url('./img/ic_mart_calendar_bg.png') top center no-repeat, transparent;background-size: 45px 52px, 100% 100%; }",
    "SMART_NEWS": "\r\n\r\n.SMART_NEWS{color: #ffffff;  font-size:21px;}\r\n.SMART_NEWS tr{height: 64px;}\r\n.SMART_NEWS td { border-bottom: 1px solid rgba(255,255,255,0.1); padding: 0px 30px; text-align: left;}\r\n.SMART_NEWS .TOUCH {background: rgba(0,0,0,0.2);}",
    "SMART_NEWS.EVEN tr:nth-child(2n-1)": "\r\n.SMART_NEWS.EVEN tr:nth-child(2n-1){height: 56px; }",
