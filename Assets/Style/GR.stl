    "SMART_MENU": "\r\n\r\n.SMART_MENU{color: #ffffff;  font-size:23px;}\r\n.SMART_MENU tr{height: 100px;}\r\n.SMART_MENU td { border-bottom: 1px solid rgba(255,255,255,0.1); padding: 0px 110px; text-align: left;}",
    "SMART_MEMO": "\r\n\r\n.SMART_MEMO{color: #22223b;  font-size:20px; }\r\n.SMART_MEMO table > tbody > tr{height: 56px;background-color: #fff; }\r\n.SMART_MEMO table > tbody > tr:first-child > td{border-top: solid 1px #c6c6ce;padding: 0 20px;}\r\n.SMART_MEMO table > tbody > tr:last-child > td{border-bottom: solid 1px #c6c6ce;vertical-align: top; padding-top: 15px;}\r\n.SMART_MEMO table > tbody > tr > td{border-bottom: solid 1px #c6c6ce; text-align: left; padding: 20px;}\r\n.SMART_MEMO table > tbody > tr > td:last-child{border-right: none; }\r\n.SMART_MEMO table > tbody > tr > td:nth-child(2n-1){color:#73747f; background-color: #f1f2f5; text-align:center; font-size:16px;}",
    "SMART_HEIGHT100 tr": "\r\n\r\n.SMART_HEIGHT100 tr{height: 100px;}",
    "SETTING": "\r\n\r\n/* 환경설정 */\r\n.SETTING{color: #22223b;  font-size:23px;}\r\n.SETTING tr{height: 115px;}\r\n.SETTING td { border-bottom: 1px solid #c6c6ce; padding: 0px 20px; text-align: left;}",
    "SETTIG_DEVICE": "\r\n\r\n.SETTIG_DEVICE{color: #22223b;  font-size:23px; }\r\n.SETTIG_DEVICE tr{height: 55px;background-color: #fff; }\r\n.SETTIG_DEVICE tr:first-child > td{border-top: solid 1px #c6c6ce;}\r\n.SETTIG_DEVICE tr:last-child > td{border-bottom: solid 1px #c6c6ce;vertical-align: top; }\r\n.SETTIG_DEVICE tr:first-child > td:last-child{font-size:19px; }\r\n.SETTIG_DEVICE tr > td{border-bottom: solid 1px #e4e5ec; text-align: left; padding: 0 20px;}\r\n.SETTIG_DEVICE tr > td:last-child{border-right: none; }\r\n.SETTIG_DEVICE tr > td:nth-child(2n-1){color:#73747f; background-color: #f7f7fc; text-align:center; font-size:16px;}",
    "HEIGHT115": "\r\n.HEIGHT115 {height: 115px !important;}",
    "CARE": "\r\n\r\n/*고객관리 2018 수정 최세열*/\r\n.CARE{color: #555;  font-size:26px;}\r\n.CARE > tr{height: 100px;}\r\n.CARE > tr > td{padding-left:18px; border-bottom: solid 1px #dadada; text-align: left;}\r\n.CARE > tr > td:last-child{border-right: none; background:#fff url('./img/img_arrow.png') 95% 50% no-repeat; background-size: 13px 23px; }\r\n.CARE > tr > td:nth-child(2n-1){text-align:center; font-size:26px; color:#797979;}\r\n.CARE .SELECT {background-color:#f5f5f5;}\r\n.CARE > tr > td:last-child.SELECT {background:#f5f5f5 url('./img/img_arrow.png') 95% 50% no-repeat;}",
    "FONT18_COLS2N1 tr > td:nth-child(2n-1)": "\r\n\r\n\r\n\r\n/*주식잔고 및 선물옵션잔고에 필요한 CSS 정의*/\r\n.FONT18_COLS2N1 tr > td:nth-child(2n-1){font-size:14px;}",
