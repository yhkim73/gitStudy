    "WHITE": "\r\n\r\n/* white  ===============================*/\r\n.WHITE {background:#f7f7fc;}\r\n.WHITE > tr > td{border-bottom: solid 1px #FFFFFF;border-right: solid 1px #FFFFFF;}\r\n.WHITE > tr > td:last-child{border-right: none;}",
    "WHITE_NAME_BG": "\r\n.WHITE_NAME_BG{background:#f2f2f7;}",
    "WHITE_COLOR1 tr > td:nth-child(1)": "\r\n.WHITE_COLOR1 tr > td:nth-child(1){background-color: #f2f2f7;}",
    "WHITE_COLOR2 tr > td:nth-child(2)": "\r\n.WHITE_COLOR2 tr > td:nth-child(2){background-color: #f2f2f7;}",
    "NAVY": "\r\n\r\n/* navy  ===============================*/\r\n.NAVY {color: #FFFFFF; background:transparent;}\r\n.NAVY > tr > td{border-bottom: solid 1px #373e57;border-right: solid 1px #373e57;}\r\n.NAVY > tr > td:last-child{border-right: none;}\r\n.NAVY > tr:last-child > td {border-bottom: solid 1px #373e57;}",
    "SMART": "\r\n\r\n/* smart  ===============================*/\r\n.SMART {color: #FFFFFF; background:rgba(0,0,0, 0.1);}\r\n.SMART > tr:first-child {background:rgba(0,0,0, 0.2); }\r\n.SMART > tr:nth-child(1) ~ tr > td:nth-child(1){background:rgba(0,0,0, 0.2);}\r\n.SMART > tr > td{border-bottom: solid 1px rgba(255,255,255,0.03);border-right: solid 1px rgba(255,255,255,0.03);}\r\n.SMART > tr > td:last-child{border-right: none;}\r\n.SMART > tr:last-child > td {border-bottom: none;}",
    "BAR_B_R": "\r\n\r\n/* 막대 그래프 */\r\n.BAR_B_R {background: -webkit-linear-gradient(#bee2ff, #bee2ff) left bottom no-repeat;background-size: 90% 7px; }",
    "BAR_B_L": "\r\n.BAR_B_L {background: -webkit-linear-gradient(#bee2ff, #bee2ff) right bottom no-repeat;background-size: 20% 7px; }",
    "BAR_R_R": "\r\n.BAR_R_R {background: -webkit-linear-gradient(#ffccd1, #ffccd1) left bottom no-repeat;background-size: 30% 7px;}",
    "BAR_R_L": "\r\n.BAR_R_L {background: -webkit-linear-gradient(#ffccd1, #ffccd1) right bottom no-repeat;background-size: 60% 7px;}",
    "G_LINE_R": "\r\n\r\n/* 짧은 오른선*/\r\n.G_LINE_R {background: -webkit-linear-gradient(#e4e5ec, #e4e5ec) right center no-repeat, transparent ; background-size: 1px 18px, 100% 100%; }",
    "LINE_2N > tr:nth-child(2n) > td": "\r\n\r\n/* 줄단위로 진함*/\r\n.LINE_2N > tr:nth-child(2n) > td{border-bottom: solid 1px #c6c6ce;}",
    "LINE_3N > tr:nth-child(3n) > td": "\r\n.LINE_3N > tr:nth-child(3n) > td{border-bottom: solid 1px #c6c6ce;}",
