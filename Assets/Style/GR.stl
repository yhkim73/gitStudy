{
  "styleName": "GR",
  "items": {
    "HEAD_ROUND": "/*COMMON*/\r\n/* HEAD ===============================*/\r\n.HEAD_ROUND{ font-size:22px; font-family:'Semibold'; }\r\n\r\n.HEAD_ROUND > tr > td{background:url('./img/table_l.png') left center no-repeat, url('./img/table_r.png') right center no-repeat, url('./img/table_c.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; color: #7a7c8b; height: 55px; border-top: solid 1px #c6c6ce; border-bottom: solid 1px #e4e5ec;}\r\n\r\n/* HEAD에 라운드 제거 */\r\n.HEAD_ROUND .TITLE_N {background: #f7f7fc;}\r\n\r\n.HEAD_ROUND .G{background:url('./img/table_l_g.png') left center no-repeat, url('./img/table_r_g.png') right center no-repeat, url('./img/table_c_g.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; color: #7a7c8b; height: 55px;}\r\n.HEAD_ROUND .P{background:url('./img/table_l_p.png') left center no-repeat, url('./img/table_r_p.png') right center no-repeat, url('./img/table_c_p.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; color: #7a7c8b; height: 55px;}\r\n.HEAD_ROUND .Y{background:url('./img/table_l_y.png') left center no-repeat, url('./img/table_r_y.png') right center no-repeat, url('./img/table_c_y.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; color: #7a7c8b; height: 55px;}\r\n.HEAD_ROUND .GR_AddFlag{background:url('./img/table_l.png') left center no-repeat, url('./img/table_r2.png') right center no-repeat, url('./img/table_c.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; }",
    "SHOW_COL_1 > tr > td:nth-child(1)": "\r\n\r\n.SHOW_COL_1 > tr > td:nth-child(1) {border-right: solid 1px #e4e5ec;}",
    "HEAD_LINE": "\r\n\r\n.HEAD_LINE { font-size:22px; font-family:'Semibold'; }\r\n.HEAD_LINE > tr > td{background: #f7f7fc; border-bottom: solid 1px #e4e5ec;border-right: solid 1px #e4e5ec; color: #7a7c8b; border-top: solid 1px #c6c6ce;}\r\n.HEAD_LINE > tr > td:last-child{border-right: none;  border-top: solid 1px #c6c6ce;}",
    "HEAD_LINE_NO": "\r\n\r\n.HEAD_LINE_NO { font-size:22px; font-family:'Semibold'; }\r\n.HEAD_LINE_NO > tr > td{background: #f7f7fc; color: #7a7c8b; height: 55px;  border-top: solid 1px #c6c6ce;}",
    "HEAD_H40 > tr > td": "\r\n.HEAD_H40 > tr > td{height: 40px;}",
    "HEAD_TOP_NO > tr > td": "\r\n.HEAD_TOP_NO > tr > td{border-top: none;}",
    "GR_AddFlag": "\r\n.GR_AddFlag{background:url('./img/table_l.png') left center no-repeat, url('./img/table_r2.png') right center no-repeat, url('./img/table_c.png') center center repeat-x, #f7f7fc;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100%; }",
    "GR_NoFlag": "\r\n.GR_NoFlag{background:url('./img/table_l.png') left center no-repeat, url('./img/table_r.png') right center no-repeat, url('./img/table_c.png') center center repeat-x, #f7f7fc ;background-size: 25px 55px, 25px 55px, 25px 55px, 100% 100% ; }",
    "AddFlag": "\r\n.AddFlag{background:url('./img/ic_switch.png') right bottom no-repeat, #f7f7fc;background-size: 10px 10px, 100% 100%;}",
    "HEAD_SHADOW::after": "\r\n\r\n/* 그림자 */\r\n.HEAD_SHADOW::after{content:\"\";position:absolute;left: 0px;width: 100%;height:8px;background:url('./img/graph_shadow.png') center center repeat-x ;background-size: 8px 8px;}",
    "HEAD_LINE.SHOW_COLS_R > tr > td:last-child": "\r\n.HEAD_LINE.SHOW_COLS_R > tr > td:last-child {border-right: solid 1px #e4e5ec;}",
    "TD_COLOR_P > tr > td": "\r\n\r\n.TD_COLOR_P > tr > td{color: #22223b;}",
    "BODY": "\r\n\r\n/* BODY ===============================*/\r\n.BODY{color: #22223b; font-size:25px;}\r\n.BODY > tr { padding: 0px 10px; }\r\n\r\n.BODY > tr > td{border-bottom: solid 1px #e4e5ec;border-right: solid 1px #e4e5ec;}\r\n.BODY > tr > td:last-child{border-right: none;}\r\n.BODY > tr:last-child > td{border-bottom: solid 1px #c6c6ce;}\r\n.BODY .BORDER_ADD {border-bottom: solid 1px #c6c6ce;}\r\n\r\n/* 라인 숨기기*/\r\n.BODY .HIDE_R_BOX {border-right: none;}\r\n.BODY .HIDE_B_BOX {border-bottom: none;}\r\n.BODY .SHOW_B_BOX {border-bottom: solid 1px #c6c6ce;}",
    "BODY.HIDE_R_COLS_1 > tr > td:nth-child(1)": "\r\n.BODY.HIDE_R_COLS_1 > tr > td:nth-child(1){border-right: none;}",
    "BODY.HIDE_R_COLS_3 > tr > td:nth-child(3)": "\r\n.BODY.HIDE_R_COLS_3 > tr > td:nth-child(3){border-right: none;}",
    "BODY.HIDE_R_ROW1_COLS1 > tr:nth-child(2n-1) > td:nth-child(1)": "\r\n.BODY.HIDE_R_ROW1_COLS1 > tr:nth-child(2n-1) > td:nth-child(1){border-right: none;}",
    "BODY.HIDE_A > tr > td": "\r\n.BODY.HIDE_A > tr > td{border: none;}",
