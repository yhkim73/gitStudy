    "ADropBox-Style.combo": "\r\n/*2018줄 까지 기존 소스*/\r\n\r\n/*2018 written by Choi Se Yeol*/\r\n/*Only Bottom Line Drop Box*/\r\n.ADropBox-Style.combo {border-bottom:1px solid #999;}\r\n.ADropBox-Style.combo .dropboxLabel {width:85%; border:0; /*padding-left:14px;*/ padding:0 65px 0 0; font-size:24px; color:#555; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo .dropboxButton {right:14px; background-image:url(./img/btn_drop.png);}",
    "ADropBox-Style.combo.left .dropboxLabel": "\r\n.ADropBox-Style.combo.left .dropboxLabel {padding:0 65px 0 0; text-align:left;}",
    "ADropBox-Style.combo_drop": "\r\n\r\n.ADropBox-Style.combo_drop {border-bottom:1px solid #999;}\r\n.ADropBox-Style.combo_drop .dropboxLabel {width:85%; border:0; /*padding-left:14px;*/ padding:0 65px 0 0; font-size:24px; color:#555; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo_drop .dropboxButton {right:14px; background-image:url(./img/btn_up.png);}",
    "ADropBox-Style.combo_dis": "\r\n\r\n/*Only Bottom Line Drop Box Disabled*/\r\n.ADropBox-Style.combo_dis {border-bottom:1px solid #999;}\r\n.ADropBox-Style.combo_dis .dropboxLabel {width:85%; border:0; /*padding-left:14px;*/ padding:0 65px 0 0; font-size:24px; color:#999; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo_dis .dropboxButton {right:14px; background-image:url(./img/btn_drop_dis.png);}",
    "ADropBox-Style.combo2": "\r\n\r\n/*Border Drop Box*/\r\n.ADropBox-Style.combo2 {border-bottom:1px solid #cfcfcf;}\r\n.ADropBox-Style.combo2 .dropboxLabel {width:85%; padding:0 10px 0 25px; font-size:26px; color:#555; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo2 .dropboxButton {background:url(./img/btn_drop.png) no-repeat 100% 100%;}",
    "ADropBox-Style.combo2.lt .dropboxLabel": "\r\n.ADropBox-Style.combo2.lt .dropboxLabel {padding-left:18px;}",
    "ADropBox-Style.combo2_drop": "\r\n\r\n.ADropBox-Style.combo2_drop {border:1px solid #dadada;}\r\n.ADropBox-Style.combo2_drop .dropboxLabel {width:85%; padding:0 65px 0 14px; font-size:26px; color:#555; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo2_drop .dropboxButton {right:14px; background-image:url(./img/btn_up.png);}",
    "ADropBox-Style.combo2_dis": "\r\n\r\n/*Border Drop Box Disabled*/\r\n.ADropBox-Style.combo2_dis {border:1px solid #d5d5d5; background-color:#ebebeb;}\r\n.ADropBox-Style.combo2_dis .dropboxLabel {width:85%; padding:0 65px 0 14px; font-size:26px; color:#999; letter-spacing:-1px; text-align:left;}\r\n.ADropBox-Style.combo2_dis .dropboxButton {right:14px; background-image:url(./img/btn_drop_dis.png);}",
    "ADropBox-Style.active": "\r\n\r\n/*Bottom Red Line*/\r\n.ADropBox-Style.active {border-bottom:1px solid #e21a22;}"
  }
