  "items": {
    "DR_001": "/* DR_002와 동일하여 지금은 사용 안하는 디자인 */\r\n.DR_001{border: 2px solid #cbcbd3;border-radius: 5px;background: #ffffff;}\r\n.DR_001 input{position:absolute;width: 100%;padding: 0px 50px 0px 15px;color: #81818d; font-size:24px; font-family:'SemiBold'; -webkit-box-sizing: border-box;}\r\n.DR_001 span{position:absolute;background: url('./img/btn_drop.png') center center no-repeat;background-size: 20px 15px;width: 40px;height:100%;}",
