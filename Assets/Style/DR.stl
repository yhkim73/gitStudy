    "DR_S002_UP": "\r\n\r\n.DR_S002_UP\r\n{\r\n\tbackground: #333a59;\r\n\tborder-radius: 0px 0px 5px 5px;\r\n\tborder-left: 2px solid #5c5d80;\r\n\tborder-right: 2px solid #5c5d80;\r\n\tborder-bottom: 2px solid #5c5d80;\r\n}",
